<?xml version="1.0" encoding="UTF-8"?>
<xsl:stylesheet  version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
	xmlns:xalan="http://xml.apache.org/xslt">
	<xsl:output method="xml" indent="yes" xalan:indent-amount="2" />
	<xsl:template match="/ResourcePools">
		<MultiApi>
			<xsl:for-each select="ResourcePool">
				<API FlowName="" IsExtendedDbApi="" Name="overrideResourcePoolCapacity" Version="">
					<Input>
						<OverrideResourcePoolCapacity>
							<ResourcePools>
								<ResourcePool>
									<xsl:attribute name="ResourcePoolKey">
										<xsl:value-of select="@ResourcePoolKey" />
									</xsl:attribute>
									<ServiceSlots>
										<ServiceSlot>
											<Dates>
												<Date Date="2013-07-21" Capacity="1" />
											</Dates>
										</ServiceSlot>
									</ServiceSlots>
								</ResourcePool>
							</ResourcePools>
						</OverrideResourcePoolCapacity>
					</Input>
				</API>
			</xsl:for-each>
		</MultiApi>
	</xsl:template>
</xsl:stylesheet>